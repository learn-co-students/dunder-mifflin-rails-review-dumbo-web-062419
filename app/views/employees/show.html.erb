<h1> Employee's Show Page</h1>

<ul>
    <li>
      <h3> Name: <%= @employee.full_name%></h3>
    </li>
    <li>
      <h3> Alias: <%= @employee.alias%></h3>
    </li>
    <li>
      <h3> Title: <%= @employee.title%></h3>
    </li>
    <li>
      <h3> Office: <%= @employee.office%></h3>
    </li>
    <li>
      <h3> Img_url: <img src=<%= @employee.img_url%> height="200" width="200"></h3>
    </li>
    <li>
      <h3> Dog's Name: <%= @employee.dog.name %></h3>
    </li>

</ul>

<h3><%= link_to "Edit Employee", edit_employee_path%> | <%= link_to "All employees",employees_path%>
<%= button_to "Delete Employee", employee_path(@employee), method: :delete%>