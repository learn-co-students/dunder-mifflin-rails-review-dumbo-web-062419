<h1>Viewing list of employees</h1>
<p>
  <%= link_to "Add new employee", new_employee_path %> |
  <%= link_to "View dogs", dogs_path %>
</p>
<hr>
<ul>
  <% @employees.each do |employee| %>
    <li><%= link_to employee.first_name + " " + employee.last_name, employee_path(employee) %></li>
  <% end %>
</ul>